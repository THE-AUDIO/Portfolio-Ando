{
    "version": 2,
    "builds": [
      {
        "src": "dist/andorica/server/server.mjs",
        "use": "@vercel/node"
      },
      {
        "src": "dist/andorica/browser/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      }
    ]
  }
  